				alert('button clicked');
			});

			$('#editModal').on('show.bs.modal', function (event) {
				var modal = $(this);
				var chk = $('input[type=radio]:checked').val();

				if (typeof (chk) === "undefined") {
					alert('請先確認修改項目！');
					location.reload();
					return false;
				}

				if (toEdit == 0) {

					@foreach (Case_informatio ca in case4)
					{

						@:if (@ca.index == chk){
							@:toEdit = 1;
							@*@:modal.find('#id').val(@ca.index);*@
							@:modal.find('#lastname').val('@ca.name');
							// modal.find('#lastname').val(data.result.name);
							@:modal.find('#gender').val('@ca.six');
							@:modal.find('#birthday').val('@ca.birthday');
							@:modal.find('#pid_last_5').val('@ca.identity');
							@:modal.find('#phone').val('@ca.telephone');
							@:modal.find('#economic_class').val('@ca.economic');
							@:modal.find('#obstacle_category').val('@ca.obstacleclass');
							@:modal.find('#obstacle_degree').val('@ca.obstacledegree');
							@:modal.find('#contact_person').val('@ca.contact');
							@:modal.find('#contact_mobile').val('@ca.phone');
							@:modal.find('#note_date').val('@ca.notedate');
							@:modal.find('#cms_class').val('@ca.CMS');
							@:modal.find('#note_unit').val('@ca.nateunit');
							@:modal.find('#familypart').val('@ca.waiter');
							@:modal.find('#status').val('@ca.status');
							@:modal.find('#no_service_reason').val('@ca.no_service_reason');
							@:modal.find('#no_service_date').val('@ca.no_service_date');


						@:addDetail(@Int32.Parse(ca.BAnum) - 1);
						int j = 0;
						for (int i = 0; i < Int32.Parse(ca.BAnum); i++)
						{
							@:modal.find('#note_item1-' + @j).val('@ca.BAList[i]');
							@:modal.find('#note_count1-' + @j).val('@ca.BAList[i + 10]');
							j += 1;
						}
						@:}
					}
				}
						});



